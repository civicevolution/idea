Changed end of haproxy, do this in chef

#listen cluster :80
#  server app-0 ec2-50-18-126-194.us-west-1.compute.amazonaws.com:81 check inter 5000 fastinter 1000 fall 1 weight 50

frontend public
        bind :80
        monitor-uri     /monitoruri

        acl nodejs path_dir socket.io
        acl nodejs_init path /application.js /index.html

        use_backend nodejs_server if METH_CONNECT or nodejs or nodejs_init
        default_backend webserver

backend nodejs_server
        server nodejs-0 127.0.0.1:8080 check inter 5000 fastinter 1000 fall 1 weight 50

backend webserver
        mode http
        server app-0 127.0.0.1:81 check inter 5000 fastinter 1000 fall 1 weight 50


listen clusterssl :443
  mode tcp
  server app-0 ec2-50-18-126-194.us-west-1.compute.amazonaws.com:444 check inter 5000 fastinter 1000 fall 1 weight 50
