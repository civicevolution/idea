%div.themer_themes
	%style
		:sass
			div.themer_themes
				div.inner
					overflow: auto
					overflow-y: scroll
					div.live_talking_point
						padding-left: 24px
						width: 450px
					div.live_talking_point.example
						background: url("/images/star_sprite.gif") no-repeat -20px -20px

	%div.inner
		= render :partial => 'ce_live/live_theme', :collection => @live_themes, :locals =>{:mode => 'left_side_list'}